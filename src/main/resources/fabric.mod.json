{
  "schemaVersion": 1,
  "id": "inhibitor",
  "version": "${version}",
  "name": "Inhibitor",
  "description": "A small mod that gives you the option to stop wynntils from scanning your player data or connecting you to any services",
  "authors": ["_GoldenShadow"],
  "contact": {
    "sources": "https://github.com/GoldenShad0w/Inhibitor",
    "issues": "https://github.com/GoldenShad0w/Inhibitor/issues"
  },
  "license": "MIT",
  "icon": "assets/inhibitor/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "goldenshadow.inhibitor.client.InhibitorClient"
    ],
    "main": [
      "goldenshadow.inhibitor.Inhibitor"
    ],
    "modmenu": [
      "goldenshadow.inhibitor.client.ModMenuIntegration"
    ]
  },
  "mixins": [
    "inhibitor.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}",
    "cloth-config" : "*",
    "modmenu" : "*"
  }
}
